{
DEVICES
{
	A1 = AND(2);
	A2 = AND(3);
	N1 = NAND(2);
	SW = SW;
	CLK1 = CLK;
}

INIT{
        SW = 1;
        CLK1 = 4;
}

CONNECTIONS
{
	A1.1 <= N1.O;
	A1.2 <= I1.O;
	A2.1 <= I1.0;
	A2.2 <= A1.O;
	A2.3 <= N1.NO;  /*Comments can /* be nested.*/*/
	N1.1 <= A2.O;
	N1.2 <= A1.O;
	I1   <= SW.O;

}

MONITORS
{
	OUTPUT <= I1.O; 
	CLK    <= A1.O;
}
}
